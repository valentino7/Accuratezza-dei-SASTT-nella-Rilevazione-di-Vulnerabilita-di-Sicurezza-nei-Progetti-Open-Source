<violation beginline="1758" endline="1758" begincolumn="24" endcolumn="27" rule="LocalVariableNamingConventions" ruleset="Code Style" package="org.apache.commons.compress.compressors.bzip2" class="BZip2CompressorOutputStream" method="generateMTFValues" variable="ll_i" externalInfoUrl="https://pmd.github.io/pmd-6.44.0/pmd_rules_java_codestyle.html#localvariablenamingconventions" priority="1">
